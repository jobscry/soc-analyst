{
    "description": "Create/Update  IP List",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "minLength": 3,
            "maxLength": 50,
            "pattern": "^[a-z0-9\\-_]{3,}$"
        },
        "description": {
            "type": "string"
        },
        "is_active": {
            "type": "boolean"
        },
        "is_public": {
            "type": "boolean"
        }
    },
    "required": [
        "name"
    ]
}